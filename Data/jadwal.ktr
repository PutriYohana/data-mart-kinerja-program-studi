    <py_script>import pandas as pd

def cleaning(df_in, hari):
    df_out = df_in.copy()
    df_out.columns = ["sesi", "waktu", "kode_mata_kuliah", "nama_mata_kuliah", 
                      "kegiatan", "kelas", "luring", "daring"]
    df_out = df_out[df_out["kode_mata_kuliah"].notna() &amp; \
                    (df_out["luring"].notna() | df_out["daring"].notna())]
    df_out = df_out.reset_index(drop=True)

    list_bentukpembelajaran = []
    for t in df_out.itertuples():
        if (pd.isna(t.luring) == True) &amp; (pd.isna(t.daring) == False):
            list_bentukpembelajaran.append("daring")
        elif (pd.isna(t.luring) == False) &amp; (pd.isna(t.daring) == True):
            list_bentukpembelajaran.append("luring")
        else:
            list_bentukpembelajaran.append("hybrid")

    dict_lookup_kegiatan = {"kegiatan": {"RKM": "Ruangan Kolaboratif Malam",
                                         "PM": "Praktikum Mandiri",
                                         "P": "Praktikum",
                                         "T": "Teori"}}
    df_out = df_out.replace(dict_lookup_kegiatan)

    df_out["bentuk_pembelajaran"] = list_bentukpembelajaran
    df_out = df_out.drop(["luring", "daring"], axis=1)
    df_out["hari"] = hari
    cols = df_out.columns.tolist()
    cols = cols[-1:] + cols[:-1]
    df_out = df_out[cols]

    return df_out
    
sumber = "D:/Materi Kuliah/Semester 8/TA2/JADWAL PERKULIAHAN SEMESTER GENAP T.A 21_22.xlsx"
df_raw = pd.read_excel(open(sumber, "rb"),
                       sheet_name="Jadwal",
                       index_col=None,
                       header=3)
df_senin = cleaning(df_raw.iloc[:, [0,1,2,3,4,11,12,13]], "Senin")
df_selasa = cleaning(df_raw.iloc[:, [0,1,2,3,4,22,23,24]], "Selasa")
df_rabu = cleaning(df_raw.iloc[:, [0,1,2,3,4,33,34,35]], "Rabu")
df_kamis = cleaning(df_raw.iloc[:, [0,1,2,3,4,44,45,46]], "Kamis")
df_jumat = cleaning(df_raw.iloc[:, [0,1,2,3,4,55,56,57]], "Jumat")
frames = [df_senin, df_selasa, df_rabu,
          df_kamis, df_jumat]
df_jadwal_kuliah = pd.concat(frames)

</py_script>
